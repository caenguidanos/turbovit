{
   "$schema": "https://turborepo.org/schema.json",
   "pipeline": {
      "build": {
         "dependsOn": ["^build"],
         "outputs": ["dist/**"]
      },
      "lint": {
         "outputs": []
      },
      "dev": {
         "dependsOn": ["^build"],
         "cache": false
      },
      "app#dev": {
         "dependsOn": ["style#lint"],
         "cache": false
      }
   }
}
