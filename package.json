{
   "name": "monorepo-template",
   "version": "0.0.0",
   "private": true,
   "workspaces": [
      "apps/*",
      "packages/**/*"
   ],
   "scripts": {
      "build": "turbo run build",
      "build:style": "turbo run build --scope=style",
      "dev": "turbo run dev --parallel",
      "format": "prettier --write .",
      "lint": "turbo run lint",
      "lint:style": "turbo run lint --scope=style",
      "storybook": "start-storybook -p 6006",
      "storybook:build": "build-storybook"
   },
   "dependencies": {
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "valtio": "1.3.0"
   },
   "devDependencies": {
      "@babel/core": "7.17.5",
      "@stitches/react": "1.2.6",
      "@storybook/addon-a11y": "6.4.19",
      "@storybook/addon-actions": "6.5.0-alpha.41",
      "@storybook/addon-essentials": "6.5.0-alpha.41",
      "@storybook/addon-storysource": "6.4.19",
      "@storybook/react": "6.5.0-alpha.41",
      "@storybook/testing-library": "0.0.9",
      "@types/node": "17.0.19",
      "@types/react": "17.0.39",
      "@types/react-dom": "17.0.11",
      "@vitejs/plugin-react": "1.2.0",
      "autoprefixer": "10.4.2",
      "babel-loader": "8.2.3",
      "postcss": "8.4.6",
      "postcss-import": "14.0.2",
      "postcss-message-helpers": "2.0.0",
      "postcss-nesting": "10.1.2",
      "postcss-scss": "4.0.3",
      "postcss-url": "10.1.3",
      "prettier": "2.5.1",
      "rollup-plugin-copy": "3.4.0",
      "sass": "1.49.8",
      "storybook-builder-vite": "0.1.17",
      "stylelint": "14.5.2",
      "stylelint-config-prettier-scss": "0.0.1",
      "stylelint-config-standard-scss": "3.0.0",
      "tailwindcss": "3.0.22",
      "turbo": "latest",
      "typescript": "4.5.5",
      "vite": "2.8.4",
      "vite-plugin-dts": "0.9.9",
      "vitest": "0.5.3"
   },
   "engines": {
      "npm": ">=8.0.0",
      "node": ">=16.0.0"
   },
   "packageManager": "npm@8.5.0"
}
